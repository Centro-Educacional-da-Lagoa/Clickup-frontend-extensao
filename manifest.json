{
  "name": "ClickUp",
  "description": "Migra tarefas em 'complete' para 'fechado'",
  "version": "1.2.0",
  "manifest_version": 3,
"key": "",
"permissions": ["storage", "alarms"],
  "host_permissions": [
    "https://api.clickup.com/*",
    "https://app.clickup.com/*",
    "http://localhost:3000/*"
  ],
  "background": { "service_worker": "service_worker.js" },

  "action": {
    "default_title": "ClickUp Manager",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },

  "content_scripts": [
    {
      "matches": ["https://app.clickup.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "options_page": "options.html",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
